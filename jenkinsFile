pipeline{
    agent any
        stages{
            stage('Build Backend'){
                steps{
                    bat 'npm install'
                }
            }
            stage('Build front-end'){
                steps{
                    bat 'cd FrontEnd && npm install'
                }
            }
            stage('Test Backend'){
                steps{
                    bat 'npm run test'
                }
            }
            stage('Test FrontEnd'){
                steps{
                    bat 'npm run front'
                }
            }
        }
    
}