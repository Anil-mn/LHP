pipeline{
    agent any
        stages{
            stage('Build Backend'){
                steps{
                    bat 'cd Backend && npm install'
                }
            }
            stage('Build front-end'){
                steps{
                    bat 'cd Backend && npm install'
                }
            }
            stage('Test Backend'){
                steps{
                    bat 'cd Backend && npm jest'
                }
            }
            stage('Test FrontEnd'){
                steps{
                    bat 'cd FrontEnd && npm test'
                }
            }
        }
    
}